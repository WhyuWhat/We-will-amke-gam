script/source = "extends CharacterBody2D

# Movement variables
@export var speed: float = 200.0
@export var jump_velocity: float = -400.0
@export var gravity: float = 1000.0
@export var dash_power: float = 600.0
@export var crouch_speed: float = 100.0

# State variables
var can_dash: bool = true
var is_crouching: bool = false
var facing_direction: float = -1.0  # Default facing left

# Nodes
@onready var animation_player: AnimationPlayer = $AnimationPlayer
@onready var sprite: Sprite2D = $Sprite2D
@onready var collision_stand: CollisionShape2D = $CollisionStand
@onready var collision_crouch: CollisionShape2D = $CollisionCrouch

func _physics_process(delta: float) -> void:
	handle_movement(delta)
	handle_animations()
	move_and_slide()

func handle_movement(delta: float) -> void:
	# Apply gravity
	if not is_on_floor():
		velocity.y += gravity * delta
	else:
		can_dash = true

	# Horizontal movement
	var horizontal_input = Input.get_axis(\"ui_left\", \"ui_right\")
	
	if not is_crouching and not is_dashing():
		velocity.x = horizontal_input * speed
		if horizontal_input != 0:
			facing_direction = sign(horizontal_input)

	# Jump
	if Input.is_action_just_pressed(\"ui_jump\") and is_on_floor() and !is_crouching:
		velocity.y = jump_velocity

	# Crouch
	if Input.is_action_pressed(\"ui_crouch\") and is_on_floor():
		is_crouching = true
		collision_stand.disabled = true
		collision_crouch.disabled = false
		velocity.x = horizontal_input * crouch_speed
	else:
		is_crouching = false
		collision_stand.disabled = false
		collision_crouch.disabled = true

	# Dash
	if Input.is_action_just_pressed(\"ui_dash\") and can_dash and !is_crouching:
		velocity.x = facing_direction * dash_power
		can_dash = false
		await get_tree().create_timer(0.4).timeout
		velocity.x = speed * facing_direction

func handle_animations() -> void:
	# Flip the sprite based on facing direction
	if facing_direction == 1:
		sprite.flip_h = true  # Flip to face right
	else:
		sprite.flip_h = false  # Default to face left

	# Play animations based on state
	if is_dashing():
		animation_player.play(\"dash\")
	elif !is_on_floor():
		if velocity.y < 0:
			animation_player.play(\"jump\")  # Rising
		else:
			animation_player.play(\"fall\")  # Falling
	elif is_crouching:
		animation_player.play(\"crouch\")
	elif abs(velocity.x) > 10:
		animation_player.play(\"walk\")
	else:
		animation_player.play(\"idle\")

func is_dashing() -> bool:
	return abs(velocity.x) >= dash_power - 100
"

